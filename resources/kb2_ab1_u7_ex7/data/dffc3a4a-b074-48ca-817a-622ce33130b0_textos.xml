<?xml version="1.0" encoding="UTF-8"?><textos><text id="ES1u6"><html><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Muli" SIZE="18" COLOR="#000000" LETTERSPACING="0" KERNING="0"><B>1</B></FONT></P></TEXTFORMAT></html></text><text id="T3FoN"><html><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Muli" SIZE="18" COLOR="#000000" LETTERSPACING="0" KERNING="0"><B>2</B></FONT></P></TEXTFORMAT></html></text><text id="BuqqW"><html><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Muli" SIZE="18" COLOR="#000000" LETTERSPACING="0" KERNING="0"><B>3</B></FONT></P></TEXTFORMAT></html></text><text id="U6QKs"><html><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Muli" SIZE="18" COLOR="#000000" LETTERSPACING="0" KERNING="0"><B>4</B></FONT></P></TEXTFORMAT></html></text><text id="CPHqP"><html><P ALIGN="LEFT"><FONT FACE="Muli" SIZE="18" COLOR="#000000" LETTERSPACING="0" KERNING="0">The elephants have got small ears.</FONT></P></html></text><text id="tjDi7"><html><P ALIGN="LEFT"><FONT FACE="Muli" SIZE="18" COLOR="#000000" LETTERSPACING="0" KERNING="0">The monkeys are on bikes.</FONT></P></html></text><text id="vGJRG"><html><P ALIGN="LEFT"><FONT FACE="Muli" SIZE="18" COLOR="#000000" LETTERSPACING="0" KERNING="0">The hippos are fat.</FONT></P></html></text><text id="FqXHB"><html><P ALIGN="LEFT"><FONT FACE="Muli" SIZE="18" COLOR="#000000" LETTERSPACING="0" KERNING="0">The tigers are next to the crocodiles.</FONT></P></html></text><text id="jIb6b"><html><P ALIGN="LEFT"><FONT FACE="Muli" SIZE="18" COLOR="#000000" LETTERSPACING="0" KERNING="0">The snakes have got legs.</FONT></P></html></text><text id="n8N17"><html><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Muli" SIZE="18" COLOR="#000000" LETTERSPACING="0" KERNING="0"><B>5</B></FONT></P></TEXTFORMAT></html></text><text id="OsLoD"><html><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Muli" SIZE="18" COLOR="#000000" LETTERSPACING="0" KERNING="0"><B>6</B></FONT></P></TEXTFORMAT></html></text><text id="PSpg4"><html><P ALIGN="LEFT"><FONT FACE="Muli" SIZE="18" COLOR="#000000" LETTERSPACING="0" KERNING="0">The giraffes are under the elephants.</FONT></P></html></text><text id="c1XEb"><html><P ALIGN="LEFT"><FONT FACE="Muli" SIZE="24" COLOR="#000000" LETTERSPACING="0" KERNING="0"><B>Read and choose.</B></FONT></P></html></text></textos>